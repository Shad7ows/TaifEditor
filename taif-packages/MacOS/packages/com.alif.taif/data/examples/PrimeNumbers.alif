دالة هل_اولي(عدد):
	اذا عدد < 2:
		ارجع
	اذا عدد == 2:
		اطبع(عدد)
		ارجع
	اذا ليس عدد \\ 2:
		ارجع
	لاجل مقسوم في مدى(3, صحيح(\^عدد) + 1, 2):
		اذا ليس عدد \\ مقسوم:
			ارجع
	اطبع(عدد)

